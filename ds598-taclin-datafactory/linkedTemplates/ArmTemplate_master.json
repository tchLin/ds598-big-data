{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ds598-taclin-datafactory"
		},
		"DS598_Github_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'DS598_Github'"
		},
		"ds598_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ds598_datalake'"
		},
		"DS598_Github_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "tchLin"
		},
		"ds598_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ds598taclin.dfs.core.windows.net/"
		},
		"ds598_keyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://openweatherAPI-taclin.vault.azure.net/"
		},
		"openweatherAPI_airPollution_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org"
		},
		"openweatherAPI_weather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/data/2.5/history/city"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ds598-taclin-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DS598_Github_credential": {
						"value": "[parameters('DS598_Github_credential')]"
					},
					"ds598_datalake_accountKey": {
						"value": "[parameters('ds598_datalake_accountKey')]"
					},
					"DS598_Github_properties_typeProperties_username": {
						"value": "[parameters('DS598_Github_properties_typeProperties_username')]"
					},
					"ds598_datalake_properties_typeProperties_url": {
						"value": "[parameters('ds598_datalake_properties_typeProperties_url')]"
					},
					"ds598_keyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ds598_keyvault_properties_typeProperties_baseUrl')]"
					},
					"openweatherAPI_airPollution_properties_typeProperties_url": {
						"value": "[parameters('openweatherAPI_airPollution_properties_typeProperties_url')]"
					},
					"openweatherAPI_weather_properties_typeProperties_url": {
						"value": "[parameters('openweatherAPI_weather_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}